---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>namespace.at system info</title>
 <link href="http://blogs.namespace.at/services/atom.xml" rel="self"/>
 <link href="http://blogs.namespace.at/services/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blogs.namespace.at/services/</id>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://blogs.namespace.at/services/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blogs.namespace.at/services/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
